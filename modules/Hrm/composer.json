{
    "name": "acmarche/hrm",
    "description": "Human Resource Management module for AcMarche",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AcMarche",
            "email": "info@acmarche.be"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "AcMarche\\Hrm\\": "src/",
            "AcMarche\\Hrm\\Database\\Factories\\": "database/factories/",
            "AcMarche\\Hrm\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AcMarche\\Hrm\\HrmServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
